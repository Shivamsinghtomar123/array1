#include <iostream>
using namespace std;

int main() {
    int t,x;
    cin>>t;
	while(t--)
	
	{   cin>>x;
	    int arr[x],count=0;
	    for(int i=0; i<x; i++)
	    {
	        cin>>arr[x];
	        if(arr[x]>=1000)
	        {
	            count++;
	            
	        }
	        
	    }	cout<<count<<endl;
	}

	return 0;
}
